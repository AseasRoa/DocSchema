{
  "name": "docschema",
  "version": "0.0.0",
  "author": {
    "name": "AseasRoa"
  },
  "description": "Schema declaration and validation library using JsDoc comments",
  "license": "MIT",
  "type": "module",
  "types": "./types/global.d.ts",
  "main": "./src/index.js",
  "browser": "./dist/bundle.js",
  "exports": {
    "types": "./types/global.d.ts",
    "browser": "./dist/bundle.js",
    "import": "./src/index.js",
    "default": "./src/index.js"
  },
  "imports": {
    "#docschema": {
      "default": "./src/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "lint": "tsc --noEmit && eslint --config .eslintrc --ext .js,.ts ./src",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "benchmark": "node benchmarks/index.js",
    "build": "vite build"
  },
  "private": false,
  "devDependencies": {
    "@types/benchmark": "^2.1.5",
    "@vitest/coverage-v8": "^0.34.6",
    "benchmark": "^2.1.4",
    "eslint": "^8.54.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-vitest": "^0.3.10",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AseasRoa/DocSchema"
  },
  "homepage": "https://github.com/AseasRoa/DocSchema",
  "keywords": [
    "validation",
    "jsdoc",
    "frontend",
    "backend"
  ]
}
